{
  "compilerOptions": {
    "lib": [
      "es5",
      "es6",
      "es7",
      "es2015",
      "es2016",
      "es2017",
      "es2018",
      "esnext"
    ],
    "target": "es5",
    "module": "esnext",
    "moduleResolution": "node",
    "rootDir": "./src",
    "outDir": "./dist",
    "declaration": true,
    "declarationMap": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "esModuleInterop": true,
    "noErrorTruncation": true,
    "jsx": "react-native",
    "skipLibCheck": true,
    "types": [],
    "noEmit": false,
    "noEmitOnError": false,
    "strict": false
  },
  "exclude": [
    "node_modules",
    "./node_modules",
    "./node_modules/*",
    "./node_modules/@types/node/index.d.ts"
  ]
}
